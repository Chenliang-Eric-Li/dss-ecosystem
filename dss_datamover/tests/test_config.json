{
    "test_remote_device": {
        "host": "msl-ssg-vm29",
        "user": "ansible",
        "password": "pass"
    },
    "clients_hosts_or_ip_addresses":["msl-ssg-vm29-tcp-0","msl-ssg-vm30-tcp-0","msl-ssg-vm31-tcp-0","msl-ssg-vm32-tcp-0"],
    "master": {
        "host_or_ip_address": "msl-ssg-vm30-tcp-0",
        "workers": 5,
        "max_index_size": 500,
        "index_data_queue_size": 50000,
        "size": "1GB"
    },
    "client": {
        "workers": 5,
        "max_index_size": 500,
        "user_id": "ansible",
        "password": ""
    },
    "message": {
        "port_index": 4000,
        "port_status": 4001
    },
    "ip_address_family": "IPV4",
    "socket_options": {
        "connection_retry_delay": 3,
        "max_connection_time_threshold": 400,
        "response_header_length": 10,
        "recv_timeout": 60
    },
    "fs_config": {
        "mounted": false,
        "nfs": {
            "192.168.200.199": ["/mnt/nfs_share/5gb"]
        }
    },
    "s3_storage": {
        "minio": {"url": "msl-ssg-vm29-tcp-0:9000", "access_key": "minio", "secret_key": "minio123"},
        "bucket": "bucket",
        "client_lib": "dss_client"
    },
    "operations": ["PUT","GET","DEL","LIST"],
    "logging": {"path": "/var/log/dss", "level": "INFO"},
    "dss_targets": {
        "subsystem_nqn": {
            "msl-ssg-vm29.msl.lab": ["nqn.2023-02.io:msl-ssg-vm29-kv_data1"],
            "msl-ssg-vm30.msl.lab": ["nqn.2023-02.io:msl-ssg-vm30-kv_data1"],
