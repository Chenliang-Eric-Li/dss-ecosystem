            "msl-ssg-vm32.msl.lab": ["nqn.2023-02.io:msl-ssg-vm32-kv_data1"]
        },
        "installation_path": "/usr/dss/nkv-target"
    },
    "environment": {
        "gcc" : {
            "version": "5.1",
            "source" : "/usr/local/bin/setenv-for-gcc510.sh",
            "required": true
        }
    },
    "aws": {
        "awslib_log_debug": 0
    }
